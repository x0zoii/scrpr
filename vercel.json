{
  "functions": {
    "api/app.py": {
      "runtime": "python@3.11"
    }
  },
  "installCommand": "pip install -r requirements.txt && python -m playwright install chromium",
  "build": {
    "env": {
      "PLAYWRIGHT_BROWSERS_PATH": "0"
    }
  },
  "routes": [
    {"src": "/api", "dest": "/api/app.py"},
    {"src": "/(.*)", "dest": "/api/app.py"}
  ]
}
